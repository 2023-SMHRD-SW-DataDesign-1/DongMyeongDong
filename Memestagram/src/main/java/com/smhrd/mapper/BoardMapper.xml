<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.mapper.BoardMapper">
	<insert id="upload" parameterType="BoardDTO">
		insert into board (board_seq, board_writer, board_content, board_date, board_img) values(num_board.nextval,
		#{board_writer}, #{board_content}, sysdate, #{board_img})
	</insert>
	
