<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.mapper.BoardMapper">
	<insert id="upload" parameterType="boardDTO">
		insert into "" values(num_board.nextval,
		#{title}, #{writer}, #{filename}, #{content}, sysdate)
	</insert>
	
	<select id="showboard" resultType="boardDTO">
	select * from "" order by ""
	</select>
	
	<select id="showone" parameterType="int" resultType="boardDTO">
	select * from "" where num= #{num}
	</select>
</mapper>